{
  "$schema": "https://proj.org/schemas/v0.7/projjson.schema.json",
  "type": "GeographicCRS",
  "name": "Longitude / Latitude (WGS 84)",
  "datum": {
    "type": "GeodeticReferenceFrame",
    "name": "WGS 84",
    "ellipsoid": {
      "name": "WGS 84",
      "semi_major_axis": 6378137,
      "inverse_flattening": 298.257223563
    }
  },
  "coordinate_system": {
    "subtype": "ellipsoidal",
    "axis": [
      {
        "name": "Longitude",
        "abbreviation": "lon",
        "direction": "east",
        "unit": {
          "type": "AngularUnit",
          "name": "Decimal Degree",
          "conversion_factor": 0.0174532925199433
        }
      },
      {
        "name": "Latitude",
        "abbreviation": "lat",
        "direction": "north",
        "unit": {
          "type": "AngularUnit",
          "name": "Decimal Degree",
          "conversion_factor": 0.0174532925199433
        }
      }
    ]
  }
}