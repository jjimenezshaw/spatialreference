{
  "$schema": "https://proj.org/schemas/v0.7/projjson.schema.json",
  "type": "GeographicCRS",
  "name": "Generic Lat/Long's, Degrees, -180 ==> +180",
  "datum": {
    "type": "GeodeticReferenceFrame",
    "name": "D_unnamed",
    "ellipsoid": {
      "name": "World Geodetic System of 1984",
      "semi_major_axis": 6378137,
      "inverse_flattening": 298.257222932867
    }
  },
  "coordinate_system": {
    "subtype": "ellipsoidal",
    "axis": [
      {
        "name": "Longitude",
        "abbreviation": "lon",
        "direction": "east",
        "unit": "degree"
      },
      {
        "name": "Latitude",
        "abbreviation": "lat",
        "direction": "north",
        "unit": "degree"
      }
    ]
  }
}